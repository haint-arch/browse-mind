{
  "manifest_version": 3,
  "name": "History Browser",
  "version": "1.0",
  "description": "Track and display browser history.",
  "permissions": [
    "microphone",
    "tabs",
    "downloads",
    "activeTab",
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "<all_urls>",
    "audioCapture"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}